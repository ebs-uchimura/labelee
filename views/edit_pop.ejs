  console.log("420: calender started")
});
  // get now time
function getTime(){
  //　現在時刻取得
  var now = new Date();
  var year = now.getFullYear();
  var mon = now.getMonth() + 1;
  var day = now.getDate() + 10;
  var date = year + "/" + mon + "/" + day;
  var jikan = date.toLocaleString();
  return jikan;
}
</script>
